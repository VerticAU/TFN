        </apex:pageBlockSection>   
    </apex:pageBlock>
    
     <apex:pageBlock >
        <apex:pageBlockSection title="Conservation Details" collapsible="false">
            <p><b>Covenantors vision:</b> {!loc03__Easement_FeeTitle__c.tfn_Covenantors_Vision__c}</p>
            <p><b>Landowner achievements:</b> {!loc03__Easement_FeeTitle__c.tfn_Land_Owner_Achievements__c}</p>
