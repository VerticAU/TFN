            <apex:pageBlockTable value="{!loc03__Easement_FeeTitle__c.Parcels__r}" var="td">
                <apex:column value="{!td.tfn_Volume__c}"/>
                <apex:column value="{!td.tfn_Folio__c}"/>
            </apex:pageBlockTable>
        </apex:pageBlockSection>
    </apex:pageBlock>

    <apex:pageBlock >
        <apex:pageBlockSection title="Deed Allowances" collapsible="false">
            <apex:pageBlockTable value="{!loc03__Easement_FeeTitle__c.Deed_Restrictions__r}" var="da">
                <apex:column value="{!da.Restriction_Type__c}"/>
                <apex:column value="{!da.Tier__c}"/>
                <apex:column value="{!da.tfn_Restriction_Description__c}"/>
                <apex:column value="{!da.tfn_Deed_Comments__c}"/>
            </apex:pageBlockTable>
        </apex:pageBlockSection>
    </apex:pageBlock>
    
    
    <apex:pageBlock >
        <apex:pageBlockSection title="Letters of Approval" collapsible="false">
